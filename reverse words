class Solution {
    String reverseWords(String S) {
        // code here 
        String arr[]=S.split("\\.");
        String s="";
        
        for(int i=arr.length-1;i>=0;i--){
            if(i!=arr.length-1)
             s=s+"."+arr[i];
            else{
                s=s+arr[i];
            }
        }
        return(s);
    }
}
